            <property name="queryText">select a.market_id,a.selection_id,a.settled_date,m.sport_name,m.region_name ,sum(a.amount) as amount 
from account_statement a,market_details m  where a.market_id=m.market_id
group by a.market_id,a.selection_id, a.settled_date, m.sport_name,m.region_name
order by a.settled_date desc</property>
