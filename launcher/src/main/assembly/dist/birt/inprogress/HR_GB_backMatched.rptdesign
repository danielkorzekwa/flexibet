            <property name="queryText">select a.market_id,a.selection_id,a.settled_date,
md.market_time, 
rs.state_name,
lb.price,lb.size_matched, lb.matched_date,
lbr.total_amount_matched,lbr.last_price_matched,lbr.total_to_back,lbr.total_to_lay,lbr.price_to_back,lbr.total_on_price_to_back,lbr.price_to_lay,lbr.total_on_price_to_lay,
sum(a.amount) as amount 
from account_statement a, market_details md, runner_state rs,last_bet lb,last_bet_runner lbr  
where 
a.market_id=md.market_id and 
a.market_id=rs.market_id and a.selection_id=rs.selection_id and 
a.market_id=lb.market_id and a.selection_id=lb.selection_id and lb.bet_type='B' and lb.bet_id=lbr.bet_id 
group by a.market_id,a.selection_id, a.settled_date, 
md.market_time, 
rs.state_name,
lb.price,lb.size_matched,lb.matched_date,
lbr.total_amount_matched,lbr.last_price_matched,lbr.total_to_back,lbr.total_to_lay,lbr.price_to_back,lbr.total_on_price_to_back,lbr.price_to_lay,lbr.total_on_price_to_lay
order by a.settled_date desc</property>
        </oda-data-set>
